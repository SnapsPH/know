{
  "name": "@darkbing/knowledge-retrieval",
  "version": "1.0.2",
  "description": "A powerful web crawler and knowledge processing toolkit for extracting and managing web content",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "knowledge-retrieval": "bin/cli.js"
  },
  "scripts": {
    "build": "rimraf dist && tsc",
    "prepare": "npm run build",
    "start": "ts-node -r dotenv/config src/interactive.ts",
    "crawl": "ts-node -r dotenv/config src/crawler.ts",
    "process": "ts-node -r dotenv/config src/knowledge_processor.ts",
    "cleanup": "ts-node -r dotenv/config src/cleanup.ts",
    "test": "jest --passWithNoTests",
    "lint": "eslint . --ext .ts",
    "prepublishOnly": "npm run build"
  },
  "files": [
    "dist",
    "bin",
    "README.md",
    "LICENSE"
  ],
  "keywords": [
    "web-crawler",
    "knowledge-extraction",
    "data-processing",
    "web-scraping",
    "ai-toolkit"
  ],
  "author": "Lodi Know",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/SnapsPH/know.git"
  },
  "homepage": "https://github.com/SnapsPH/know#readme",
  "bugs": {
    "url": "https://github.com/SnapsPH/know/issues"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "dependencies": {
    "axios": "^1.6.2",
    "cheerio": "^1.0.0-rc.12",
    "dotenv": "^16.3.1",
    "fs-extra": "^11.1.1",
    "inquirer": "^8.2.5",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/cheerio": "^0.22.33",
    "@types/fs-extra": "^11.0.4",
    "@types/inquirer": "^8.2.5",
    "@types/jest": "^29.5.8",
    "@types/node": "^20.9.0",
    "@types/yargs": "^17.0.29",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "eslint": "^8.53.0",
    "jest": "^29.7.0",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  }
}
